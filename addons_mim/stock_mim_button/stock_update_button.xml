<openerp>
	<data>
	    <record id="base.group_disponible" model="res.groups">
            <field name="name">Utilisteurs Rendre disponible</field>
        </record>
        <record id="base.group_traitement" model="res.groups">
            <field name="name">Utilisteurs Traitement</field>
        </record>
           
	    <record id="view_move_form20" model="ir.ui.view">
			<field name="name">stock.move.form.inherit20</field>
			<field name="model">stock.move</field>
			<field name="inherit_id" ref="stock.view_move_form" />
			<field name="arch" type="xml">
					<xpath expr="/form/header/button[@name='force_assign']" position="replace">
					    <button name="force_assign" states="draft,assigned,cancel,contre_mesure,confirmed,flowsheeting" string="Set Available" type="workflow" class="oe_highlight" groups="base.group_disponible" />
				  	</xpath>
			</field>
		</record>
		
	    <record id="view_move_form21" model="ir.ui.view">
			<field name="name">stock.move.form.inherit21</field>
			<field name="model">stock.move</field>
			<field name="inherit_id" ref="stock.view_move_form" />
			<field name="arch" type="xml">
					<xpath expr="/form/header/button[@name='action_done']" position="replace">
						<button name="action_done" states="draft,assigned,cancel,contre_mesure,confirmed,flowsheeting" string="Process Entirely" type="workflow" class="oe_highlight" groups="base.group_traitement" />	
					</xpath>
			</field>
		</record>
		
	    <record id="view_move_form22" model="ir.ui.view">
			<field name="name">stock.move.form.inherit22</field>
			<field name="model">stock.move</field>
			<field name="inherit_id" ref="stock.view_move_form" />
			<field name="arch" type="xml">
					<xpath expr="/form/header/button[@name='action_confirm']" position="replace">
						<button name="action_confirm" states="draft,cancel" string="Process Later" type="workflow" class="oe_highlight"/>	
					</xpath>
			</field>
		</record>
		 
	    <record id="view_move_form23" model="ir.ui.view">
			<field name="name">stock.move.form.inherit23</field>
			<field name="model">stock.move</field>
			<field name="inherit_id" ref="stock.view_move_form" />
			<field name="arch" type="xml">
					<xpath expr="/form/header/button[@string='Process Partially']" position="replace">
						<button name="293" string="Process Partially" type="action" states="assigned" groups="base.group_traitement" class="oe_highlight"/>	
					</xpath>
			</field>
		</record>
		
	     
        <!-- stock.view_move_picking_form--> 
