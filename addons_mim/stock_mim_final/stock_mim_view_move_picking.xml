<openerp>
	<data>
		<record id="view_move_picking_form3" model="ir.ui.view">
			<field name="name">stock.move.form.inherit3</field>
			<field name="model">stock.move</field>
			<field name="inherit_id" ref="stock.view_move_picking_form" />
			<field name="arch" type="xml">
					<xpath expr="/form/header/button[@name='force_assign']" position="after">
					    <button name="contre_mesure" states="draft,confirmed,assigned,flowsheeting" string="Contre mesure" class="oe_highlight" type="workflow" groups="base.group_contre_mesure"/>
						<button name="annuler_contre_mesure" states="contre_mesure" string="Annuler contre mesure" type="workflow" groups="base.group_contre_mesure"/>
						<button name="flow_sheet" states="draft,confirmed,assigned,contre_mesure" string="Fiche Débit" class="oe_highlight" type="workflow" groups="base.group_fiche_debit"/>
						<button name="flow_sheet_cancel" states="flowsheeting" string="Annuler Fiche Débit" type="workflow" groups="base.group_fiche_debit"/>
					</xpath>
					<field name="state" position="replace">
                    	<field name="state" widget="statusbar" statusbar_visible="draft,confirmed,contre_mesure,flowsheeting,assigned,done" statusbar_colors='{"waiting":"blue","confirmed":"blue"}'/>
                	</field>
			</field>
		</record>
	</data>
</openerp>

